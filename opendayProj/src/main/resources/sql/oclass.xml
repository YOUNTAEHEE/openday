<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
						"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
						
<mapper namespace="mapper.oclass">
<select id="selectClass" parameterType="Integer"
	resultType="com.kosta.openday.user.dto.OClassDTO">
	<![CDATA[
	select * from oclass
	where clsId=#{id}
	]]>
</select>

<select id="selectClassList" resultType="com.kosta.openday.user.dto.OClassDTO">
	select *
	from oclass
</select>
<insert id="insertClass" parameterType="com.kosta.openday.user.dto.OClassDTO">
	insert into oclass
				values(NEXTVAL(OCLASS_SEQ), #{clsName},
						#{clsCode}, 		#{clsDescription}, 	#{clsCurri},
					   	#{clsStar}, 		#{clsHeart}, 		#{clsStatus},
					   	#{clsCreatedAt}, 	#{clsOpenType},
					   	#{filNum}, 			#{userId}, 			#{reqId})
</insert>
</mapper>